#!/bin/sh
#

## EXTRAS ##
source "$HOME/.extras"

## THEME ##
BASE16_SHELL="$HOME/.config/base16-shell/"
[ -n "$PS1" ] && \
    [ -s "$BASE16_SHELL/profile_helper.sh" ] && \
        eval "$("$BASE16_SHELL/profile_helper.sh")"
BASE16_THEME="seti"

## TOOLS ##
export PATH=$HOME/.bin:$PATH

## ALIASES ##
source "$HOME/.aliases"

## FUNCTIONS ##
source "$HOME/.functions"

## VARIABLES ##
source "$HOME/.variables"
